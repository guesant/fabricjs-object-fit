{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": false,
    "moduleResolution": "node",
    "outDir": "./lib"
  },
  "include": ["./src"],
  "exclude": ["./lib"]
}
