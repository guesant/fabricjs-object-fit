{
  "name": "fabricjs-object-fit",
  "version": "0.0.0",
  "description": "CSS-like 'object-fit' behaviour for FabricJS.",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/guesant/fabricjs-object-fit.git",
  "author": "Gabriel R. Antunes <gabrielrodantunes@gmail.com>",
  "license": "LGPL-3.0",
  "keywords": [
    "fabricjs",
    "object-fit",
    "cover",
    "contain",
    "fill",
    "object-position"
  ],
  "scripts": {
    "build": "rm -rf lib; tsc --noEmit && rollup --config rollup.config.js",
    "dev": "rm -rf lib; rollup --config rollup.config.js --watch",
    "docs:dev": "vuepress dev docs --port 3000",
    "docs:build": "vuepress build docs",
    "docs:serve": "vuepress serve docs",
    "lint": "eslint 'src/**'",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*": [
      "npm run lint:fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.2",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/fabric": "^4.5.6",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.5",
    "prettier": "2.5.1",
    "prettier-config-standard": "^4.0.0",
    "rollup": "^2.62.0",
    "rollup-plugin-dts": "^4.1.0",
    "typedoc": "^0.22.10",
    "typedoc-plugin-markdown": "^3.11.8",
    "typescript": "^4.5.4",
    "vuepress": "^1.9.5",
    "vuepress-plugin-typedoc": "^0.10.0"
  }
}
